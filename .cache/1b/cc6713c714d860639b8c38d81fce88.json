tent {\r\n\tcolor: var(--color-white);\r\n\tmargin: 280px auto 0 auto;\r\n\tmax-width: 400px;\r\n\ttext-align: center;\r\n}\r\n\r\n.header__content button {\r\n\tmargin-top: 30px;\r\n}\r\n\r\n/* Nav component */\r\nnav {\r\n\talign-items: center;\r\n\tcolor: var(--color-white);\r\n\tdisplay: flex;\r\n\tjustify-content: space-between;\r\n\tpadding-top: 20px;\r\n}\r\n\r\nnav a {\r\n\tcolor: var(--color-white);\r\n}\r\n\r\nnav ul {\r\n\talign-items: center;\r\n\tdisplay: flex;\r\n}\r\n\r\nnav ul li {\r\n\tmargin-right: 20px;\r\n}\r\n\r\nnav ul li:last-child {\r\n\tmargin-right: 0;\r\n}\r\n\r\n/* Global Styles */\r\n.container {\r\n\tmargin: 0 auto;\r\n\tmax-width: 1200px;\r\n}\r\n\r\n.btn {\r\n\tbackground-color: var(--color-gray-button);\r\n\tborder: none;\r\n\tborder-radius: .4rem;\r\n\tcolor: var(--color-white);\r\n\tcursor: pointer;\r\n\tfont-family: var(--font-site);\r\n\tfont-size: 1rem;\r\n\tpadding: 22px 32px;\r\n\ttransition: all 300ms linear;\r\n}\r\n\r\n.btn:hover {\r\n\tbackground-color: var(--color-gray-button-hover);\r\n}\r\n\r\n.btn--small {\r\n\tpadding: 12px 32px;\r\n}\r\n\r\n/* info component */\r\n.info {\r\n\tmargin: 150px auto;\r\n\tmax-width: 75%;\r\n\ttext-align: center;\r\n}\r\n\r\n.info p {\r\n\tmargin-top: 40px;\r\n\tmax-width: 94%;\r\n}\r\n\r\n/* mid component */\r\n.mid {\r\n\tdisplay: flex;\r\n\tmargin: 112px 0;\r\n}\r\n\r\n.mid img {\r\n\theight: 433px;\r\n\twidth: 50%;\r\n}\r\n\r\n.mid__info {\r\n\talign-items: center;\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n\tjustify-content: center;\r\n\tpadding: 75px;\r\n\ttext-align: center;\r\n}\r\n\r\n.mid__info p {\r\n\tmargin: 30px 0;\r\n}\r\n\r\n.mid.inverted {\r\n\tflex-direction: row-reverse;\r\n}\r\n\r\n/* subscription component*/\r\n.subscription {\r\n\tbackground-color: var(--color-gray-light);\r\n\tpadding: 150px 0;\r\n\ttext-align: center;\r\n}\r\n\r\n.subscription p {\r\n\tmargin: 30px 0;\r\n}\r\n\r\n.subscription input {\r\n\tborder: none;\r\n\tborder: 1px solid var(--color-gray-mid);\r\n\tfont-family: var(--font-site);\r\n\tfont-size: 1rem;\r\n\tpadding: 22px 32px;\r\n\tmargin-right: 10px;\r\n}\r\n\r\n.subscription .container {\r\n\tmax-width: 60%;\r\n}\r\n\r\n/* Footer component */\r\nfooter {\r\n\tbackground-color: var(--color-gray-dark);\r\n\tcolor: var(--color-gray-light);\r\n\tpadding: 80px 0 90px 0;\r\n\ttext-align: center;\r\n}\r\n\r\nfooter p:first-child {\r\n\tmargin-bottom: 25px;\r\n}\r\n\r\nfooter a {\r\n\tcolor: var(--color-gray-button);\r\n\ttext-decoration: underline;\r\n}\r\n\r\n.pipe {\r\n\tmargin: 0 10px;\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"2201d04b84c78c68e6269c4c7879d4b0","cacheData":{"env":{}}}